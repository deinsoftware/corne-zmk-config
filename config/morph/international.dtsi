mp_i_a: morph_a {
    compatible = "zmk,behavior-mod-morph";
    label = "a";
    #binding-cells = <0>;
    mods = <(MOD_LALT|MOD_RALT)>;
    bindings = <&kp A>, <&kp RC(A)>;
    keep-mods = <(MOD_RALT)>;
};

